{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "roto",
      "camelcase": "Roto",
      "title": "Roto",
      "scope": "source.roto",
      "file-types": [
        "roto"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^roto$",
      "class-name": "TreeSitterRoto"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "BSD-3-Clause",
    "description": "A statically-typed, compiled, embedded scripting language for Rust",
    "authors": [
      {
        "name": "NLnet Labs",
        "email": "routing-team@nlnetlabs.nl"
      }
    ],
    "links": {
      "repository": "https://github.com/NLnetLabs/roto"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
